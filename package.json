  },
  "homepage": "https://github.com/mrkprinc/NewsScraper#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "file-system": "^2.2.2",
    "handlebars": "^4.0.12",
    "mongoose": "^5.3.14",
    "request": "^2.88.0"
